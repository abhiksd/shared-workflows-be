<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress false positives for test dependencies -->
    <suppress>
        <notes><![CDATA[
        Spring Boot Test AutoConfigure - Only used in test scope
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring\-boot\-test\-autoconfigure@.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- Suppress H2 Database CVEs for development/test only -->
    <suppress>
        <notes><![CDATA[
        H2 Database is only used for development and testing, not in production
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.h2database/h2@.*$</packageUrl>
        <vulnerabilityName>CVE-2022-45868</vulnerabilityName>
    </suppress>
    
    <!-- Suppress known false positives for Spring Boot -->
    <suppress>
        <notes><![CDATA[
        Spring Boot Configuration Processor - Development time tool only
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring\-boot\-configuration\-processor@.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- Example suppression for Testcontainers if needed -->
    <suppress>
        <notes><![CDATA[
        Testcontainers JUnit Jupiter - Test scope only
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.testcontainers/junit\-jupiter@.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <!-- Add more suppressions as needed for your specific dependencies -->
    
</suppressions>